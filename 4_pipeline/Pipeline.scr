// Pipeline - the pipeline protocol

module Pipeline;

sig <go> "messages.Val" from "github.com/nickng/scribble-go-examples/4_pipeline/message" as Val;

// Cf. ...?
global protocol Pipeline(role W) {
  Val pipe W[1,K];
}

// Fig. 8
global protocol Foreach(role W)
{
	foreach W[I1:1,k-1], W[I2:2..k]
	{
		Val() from W[I1,I1] to W[I2,I2];
	}
}
